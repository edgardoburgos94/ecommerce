<h1>Proveedores registrados (<%=@suppliers.length%>)</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Email</th>
      <th scope="col">supplier.remember_created_at</th>
      <th scope="col">current_sign_in_at</th>
      <th scope="col">last_sign_in_at</th>
      <th scope="col">current_sign_in_ip</th>
      <th scope="col">last_sign_in_ip</th>
      <th scope="col">last_created_at</th>
      <th scope="col">updated_at</th>
      <th scope="col">sign_in_count</th>
    </tr>
  </thead>
  <tbody>
    <% @suppliers.each do |supplier|%>
      <tr>
        <th><%=supplier.email%></th>
        <td><%=supplier.remember_created_at%></td>
        <td><%=supplier.current_sign_in_at%></td>
        <td><%=supplier.last_sign_in_at%></td>
        <td><%=supplier.current_sign_in_ip%></td>
        <td><%=supplier.last_sign_in_ip%></td>
        <td><%=supplier.created_at%></td>
        <td><%=supplier.updated_at%></td>
        <td><%=supplier.sign_in_count%></td>
      </tr>
    <%end%>
  </tbody>
</table>
